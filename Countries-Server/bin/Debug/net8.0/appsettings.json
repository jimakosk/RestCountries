{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Console": {
    "FormatterName": "simple",
    "FormatterOptions": {
      "IncludeScopes": false,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss | ",
      "UseUtcTimestamp": false
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\localdbdemo;Database=CountryDb;Trusted_Connection=True;TrustServerCertificate=Yes;"
  },
  "Hangfire": {
    "IsEnabled": true,
    "Minutes": 30
  }
}
