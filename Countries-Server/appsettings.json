{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Console": {
    "FormatterName": "simple",
    "FormatterOptions": {
      "IncludeScopes": false,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss | ",
      "UseUtcTimestamp": false
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Add you connection string."
  },
  "Hangfire": {
    "IsEnabled": true,
    "Minutes": "*/30 * * * *"

  }
}
